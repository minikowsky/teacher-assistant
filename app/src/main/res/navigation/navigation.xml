<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentStudentList">
    <fragment
        android:id="@+id/fragmentStudentList"
        android:name="com.example.teacherassistant.fragment.student.FragmentStudentList"
        android:label="FragmentStudentList" >
        <action
            android:id="@+id/action_fragmentStudentList_to_fragmentSubcjectListPerStudent"
            app:destination="@id/fragmentSubcjectListPerStudent" />
    </fragment>
    <fragment
        android:id="@+id/fragment_subject_list"
        android:name="com.example.teacherassistant.fragment.subject.fragment_subject_list"
        android:label="fragment_subject_list" >
        <action
            android:id="@+id/action_fragment_subject_list_to_fragmentStudentListPerSubject"
            app:destination="@id/fragmentStudentListPerSubject" />
    </fragment>
    <fragment
        android:id="@+id/fragmentStudentListPerSubject"
        android:name="com.example.teacherassistant.fragment.subject.FragmentStudentListPerSubject"
        android:label="FragmentStudentListPerSubject" >
        <action
            android:id="@+id/action_fragmentStudentListPerSubject_to_fragmentMarkList"
            app:destination="@id/fragmentMarkList" />
        <action
            android:id="@+id/action_fragmentStudentListPerSubject_to_fragment_subject_list"
            app:destination="@id/fragment_subject_list" />
    </fragment>
    <fragment
        android:id="@+id/fragmentSubcjectListPerStudent"
        android:name="com.example.teacherassistant.fragment.student.FragmentSubcjectListPerStudent"
        android:label="FragmentSubcjectListPerStudent" >
        <action
            android:id="@+id/action_fragmentSubcjectListPerStudent_to_fragmentMarkList"
            app:destination="@id/fragmentMarkList" />
        <action
            android:id="@+id/action_fragmentSubcjectListPerStudent_to_fragmentStudentList"
            app:destination="@id/fragmentStudentList" />
    </fragment>
    <fragment
        android:id="@+id/fragmentMarkList"
        android:name="com.example.teacherassistant.fragment.FragmentMarkList"
        android:label="fragment_mark_list"
        tools:layout="@layout/fragment_mark_list" >
        <action
            android:id="@+id/action_fragmentMarkList_to_fragmentSubcjectListPerStudent"
            app:destination="@id/fragmentSubcjectListPerStudent" />
        <action
            android:id="@+id/action_fragmentMarkList_to_fragmentStudentListPerSubject"
            app:destination="@id/fragmentStudentListPerSubject" />
    </fragment>
</navigation>